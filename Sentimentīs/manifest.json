{
  "manifest_version": 3,
  "name": "sentimentÄ«s",
  "version": "1.0",
  "description": "Scrapes the page title and URL in a sidebar.",
  "permissions": ["activeTab", "sidePanel", "scripting", "tabs"],
  "host_permissions": ["<all_urls>"],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_title": "",
    "default_icon" : {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "side_panel": {
    "default_path": "sidebar.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
  {
    "resources": ["chart.js"],
    "matches": ["<all_urls>"]
  }
]
}
